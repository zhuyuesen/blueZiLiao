/* pages/chooseSong/chooseSong.wxss */
.musicList{
	width: 100%;
	margin: 44rpx auto;
}
.musicList .musicListItem{
	width: calc(100% - 20rpx);
	height: 100rpx;
	line-height: 100rpx;
	box-sizing: border-box;
	justify-content: space-between;
	padding: 28rpx 35.4rpx 36rpx 0;
	margin-left: 20rpx;
	border-bottom: 1rpx solid #D8D8D8;
}
.musicList .musicListItem image{
	width: 32.6rpx;
	height: 23rpx;
}